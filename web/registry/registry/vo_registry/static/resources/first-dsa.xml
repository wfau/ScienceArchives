<ns0:Resource xmlns:ns0="http://www.ivoa.net/xml/RegistryInterface/v1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2010-02-15T12:09:22Z" status="active" updated="2019-12-04T13:34:53Z" xsi:type="vs:CatalogService" xsi:schemaLocation="http://www.ivoa.net/xml/RegistryInterface/v1.0 http://software.astrogrid.org/schema/registry/RegistryInterface/v1.0/RegistryInterface.xsd http://www.ivoa.net/xml/VOResource/v1.0 http://software.astrogrid.org/schema/vo-resource-types/VOResource/v1.0/VOResource.xsd  http://www.ivoa.net/xml/VODataService/v1.0 http://software.astrogrid.org/schema/vo-resource-types/VODataService/v1.0/VODataService.xsd http://www.ivoa.net/xml/VOTable/v1.0 http://software.astrogrid.org/schema/vo-formats/VOTable/v1.0/VOTable.xsd http://www.ivoa.net/xml/ConeSearch/v1.0 http://software.astrogrid.org/schema/vo-resource-types/ConeSearch/v1.0/ConeSearch.xsd http://www.ivoa.net/xml/SIA/v1.0 http://software.astrogrid.org/schema/vo-resource-types/SIA/v1.0/SIA.xsd http://www.ivoa.net/xml/SSA/v0.4 http://software.astrogrid.org/schema/vo-resource-types/SSA/v0.4/SSA.xsd urn:astrogrid:schema:vo-resource-types:STAP:v1.0 http://software.astrogrid.org/schema/vo-resource-types/STAP/v1.0/STAP.xsd http://www.ivoa.net/xml/CEA/v1.0rc1 http://software.astrogrid.org/schema/vo-resource-types/CEAService/v1.0rc1/CEAService.xsd">
        <title>FIRST Survey Catalogue (03Apr11 Version)</title>
        <shortName>FIRST</shortName>
        <identifier>ivo://wfau.roe.ac.uk/first-dsa</identifier>
        <curation>
            <publisher ivo-id="ivo://wfau.roe.ac.uk">WFAU, Institute for Astronomy, University of Edinburgh</publisher>
            <creator>
                <name>Robert H. Becker, David J. Helfand, Richard L. White</name>
            </creator>
            <date>2010-02-15</date>
            <version>03Apr11 Version</version>
            <contact>
                <name>Mark Holliman</name>
                <email>msh@roe.ac.uk</email>
            </contact>
        </curation>
        <content>
            <subject>Faint Images of the Radio Sky at Twenty-cm FIRST</subject>
            <description>FIRST (Faint Images of the Radio Sky at Twenty-cm) is a project designed to produce the radio equivalent of the Palomar Observatory Sky Survey over 10,000 square degrees of the North and South Galactic Caps. The catalogue covers a total of about 9033 square degrees of sky (8422 square degrees in the north Galactic cap and 611 square degrees in the south Galactic cap.)</description>
            <referenceURL>http://sundog.stsci.edu/first/catalogs/readme.html</referenceURL>
            <type>Catalog</type>
            <contentLevel>Research</contentLevel>
            <relationship>
                <relationshipType>service-for</relationshipType>
                <relatedResource>ivo://wfau.roe.ac.uk/first-dsa/ceaApplication</relatedResource>
            </relationship>
        </content>
        <capability standardID="ivo://ivoa.net/std/ConeSearch" xsi:type="cs:ConeSearch">
            <description>firstSource table: cone search</description>
            <interface role="std" xsi:type="vs:ParamHTTP">
                <accessURL use="base">http://wfaudata.roe.ac.uk/first-dsa/DirectCone?DSACAT=FIRST&amp;DSATAB=firstSource&amp;</accessURL>
            </interface>
            <maxSR>1.500000</maxSR>
            <maxRecords>2000000</maxRecords>
            <verbosity>false</verbosity>
            <testQuery>
                <ra>16.6</ra>
                <dec>-1.8</dec>
                <sr>0.1</sr>
            </testQuery>
        </capability>
        <capability standardID="ivo://org.astrogrid/std/CEA/v1.0" xsi:type="cea:CeaCapability">
            <description>Access to two applications: general ADQL query, and asynchronous cone-search where relevant/enabled.</description>
            <interface xsi:type="cea:CECInterface">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/services/CommonExecutionConnectorService</accessURL>
            </interface>
            <managedApplications>
                <ApplicationReference>ivo://wfau.roe.ac.uk/first-dsa/ceaApplication</ApplicationReference>
            </managedApplications>
        </capability>
        <capability standardID="ivo://org.astrogrid/std/VOSI/v0.3#capabilities">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/capabilities</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://ivoa.net/std/VOSI#capabilities">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/capabilities</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://org.astrogrid/std/VOSI/v0.3#availability">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/availability</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://ivoa.net/std/VOSI#availability">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/availability</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://org.astrogrid/std/VOSI/v0.3#tables">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/tables</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://org.astrogrid/std/VOSI/v0.3#ceaApplication">
            <interface xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/VOSI/applications</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <capability standardID="ivo://ivoa.net/std/Delegation">
            <interface role="std" xsi:type="vs:ParamHTTP">
                <accessURL use="full">http://wfaudata.roe.ac.uk/first-dsa/delegations</accessURL>
                <queryType>GET</queryType>
                <resultType>application/xml</resultType>
            </interface>
        </capability>
        <coverage>
            <waveband>Radio</waveband>
        </coverage>
        <table>
            <name>firstSource</name>
            <description>Contains the parameters provided for each source in the FIRST Survey Catalog: 03Apr11 Version</description>
            <column>
                <name>fieldName</name>
                <description>Name of the coadded image containing the source (encodes the position of the field center).</description>
                <ucd>ID_IMAGE</ucd>
                <dataType arraysize="*">char</dataType>
            </column>
            <column>
                <name>seqNo</name>
                <description>sequential object number</description>
                <ucd>ID_MAIN</ucd>
                <dataType arraysize="*">char</dataType>
            </column>
            <column>
                <name>cx</name>
                <description>unit vector of spherical co-ordinates</description>
                <ucd>POS_EQ_X</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>cy</name>
                <description>unit vector of spherical co-ordinates</description>
                <ucd>POS_EQ_Y</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>cz</name>
                <description>unit vector of spherical co-ordinates</description>
                <ucd>POS_EQ_Z</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>htmID</name>
                <description>Hierarchical Triangular Mesh (HTM) index, 20 deep, for equatorial co-ordinates</description>
                <ucd>POS_GENERAL</ucd>
                <dataType>long</dataType>
            </column>
            <column>
                <name>ra</name>
                <description>J2000 Celestial Right Ascension</description>
                <unit>deg</unit>
                <ucd>POS_EQ_RA_MAIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>dec</name>
                <description>J2000 Celestial Declination</description>
                <unit>deg</unit>
                <ucd>POS_EQ_DEC_MAIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>warnFlag</name>
                <description>Warning flag indicating that the source may be a sidelobe of a nearby bright source.</description>
                <ucd>CODE_MISC</ucd>
                <dataType arraysize="*">char</dataType>
            </column>
            <column>
                <name>fPeak</name>
                <description>peak flux densities</description>
                <unit>mJy</unit>
                <ucd>PHOT_FLUX_RADIO_1.4G</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>fInt</name>
                <description>integrated flux densities</description>
                <unit>mJy</unit>
                <ucd>PHOT_FLUX_RADIO_1.4G</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>rms</name>
                <description>local noise estimate at the source position</description>
                <unit>mJy</unit>
                <ucd>INST_NOISE</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>maj</name>
                <description>major axes derived from the elliptical Gaussian model for the source after deconvolution.</description>
                <unit>arcsec</unit>
                <ucd>EXTENSION_FWHM_MAJ</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>min</name>
                <description>minor axes derived from the elliptical Gaussian model for the source after deconvolution.</description>
                <unit>arcsec</unit>
                <ucd>EXTENSION_FWHM_MIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>pa</name>
                <description>position angle (east of north) derived from the elliptical Gaussian model for the source</description>
                <unit>deg</unit>
                <ucd>POS_POS-ANG</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>fMaj</name>
                <description>major axes derived from the elliptical Gaussian model for the source before deconvolution.</description>
                <unit>arcsec</unit>
                <ucd>EXTENSION_FWHM_MAJ</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>fMin</name>
                <description>minor axes derived from the elliptical Gaussian model for the source before deconvolution.</description>
                <unit>arcsec</unit>
                <ucd>EXTENSION_FWHM_MIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>fPA</name>
                <description>position angle (east of north) derived from the elliptical Gaussian model for the source before deconvolution.</description>
                <unit>deg</unit>
                <ucd>POS_POS-ANG</ucd>
                <dataType>double</dataType>
            </column>
        </table>
    </ns0:Resource>