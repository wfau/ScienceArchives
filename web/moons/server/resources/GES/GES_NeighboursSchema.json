{
    "tables": {
        "GES.TargetXGAIADR1gaia_source": {
            "name": "GES.TargetXGAIADR1gaia_source",
            "markdown": [
                {
                    "h": "Cross-neighbours between the GES Target catalogue and the Gaia DR1 main catalogue\n"
                },
                {
                    "t": "All Gaia DR1 sources within 10 arcsec of\n"
                },
                {
                    "t": "each star in the GES Target catalogue are recorded in this cross-neighbour\n"
                },
                {
                    "t": "table.  The Target table was joined to the GAIADR1..gaia_source table to create\n"
                },
                {
                    "t": "these cross-neighbours.  Use this table for any cross-querying of GES\n"
                },
                {
                    "t": "target stars with Gaia DR1 sources.\n"
                }
            ],
            "columns": {
                "masterObjID": {
                    "name": "masterObjID",
                    "type": "bigint",
                    "size": 8,
                    "default": null,
                    "description": "The unique ID in Target (=targetID )",
                    "unified_content_descriptor": "meta.id;meta.main"
                },
                "slaveObjID": {
                    "name": "slaveObjID",
                    "type": "bigint",
                    "size": 8,
                    "default": null,
                    "description": "The unique ID of the neighbour in GAIADR1..gaia_source (=source_id)",
                    "unified_content_descriptor": "meta.id.cross"
                },
                "distanceMins": {
                    "name": "distanceMins",
                    "type": "real",
                    "size": 4,
                    "default": null,
                    "description": "Angular separation between neighbours",
                    "unit": "arcminutes",
                    "unified_content_descriptor": "pos.angDistance"
                }
            }
        },
